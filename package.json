{
  "name": "farcaster-hub-prisma-replicator",
  "version": "1.0.0",
  "description": "replicates data from farcaster hubs to a postgres database with a user-friendly schema",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx index.ts",
    "setup": "npx prisma migrate dev --name init --preview-feature",
    "reset": "npx prisma migrate reset --preview-feature",
    "dev": "next dev",
    "build": "prisma generate && next build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.10.5",
    "@prisma/client": "^5.2.0",
    "@standard-crypto/farcaster-js": "^5.6.0",
    "bufferutil": "^4.0.7",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "fastq": "^1.15.0",
    "next": "^13.4.19",
    "node-cron": "^3.0.2",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "pretty-ms": "^8.0.0",
    "prisma": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supports-color": "^9.4.0",
    "tiny-typed-emitter": "^2.1.0",
    "utf-8-validate": "^5.0.10"
  },
  "devDependencies": {
    "@types/react": "18.2.21",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2"
  }
}
